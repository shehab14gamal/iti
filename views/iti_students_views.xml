<odoo>
  <data>

      #code for iti_students ..........................................................................................

      #view.............
      <record id="students_tree_view" model="ir.ui.view">
          <field name="name">students tee view</field>
          <field name="model">iti.student</field>
          <field name="arch" type="xml">
              <tree>
                  <field name="name"/>
                  <field name="birth_date"/>
                  <field name="salary"/>
              </tree>
          </field>
      </record>
      #form...
      <record id="students_form_view" model="ir.ui.view">
          <field name="name">students form view</field>
          <field name="model">iti.student</field>
          <field name="arch" type="xml">
              <form>
                  <sheet>
                      <group>
                          <group string="Basic Info">
                              <field name="name"/>
                              <field name="birth_date"/>
                               <field name="salary"/>
                               <field name="address"/>

                          </group>
                          <group string="Extra Info">
                              <field name="accepted"/>
                               <field name="level"/>
                              <field name="gender"/>
                                <field name="login_time"/>
                               <field name="track_id"/>
                          </group>
                      </group>
                      <notebook>
                          <page string="Image"><field name="image"/></page>
                           <page string="CV"> <field name="cv"/></page>



                      </notebook>
                  </sheet>
              </form>
          </field>
      </record>

      #action..........................

     <record id="students_action" model="ir.actions.act_window">
        <field name="name">Students</field>
        <field name="res_model">iti.student</field>
     </record>
      #menus.......................................
      <menuitem id="iti_root" name="ITI"/>
     <menuitem id="iti_students_menu" name="students" parent="iti_root" action="students_action"/>

#code for iti_tracks..................................................................................................

      #view...................
        <record id="tracks_tree_view" model="ir.ui.view">
          <field name="name">tracks tee view</field>
          <field name="model">iti.track</field>
          <field name="arch" type="xml">
              <tree>
                  <field name="name"/>
                  <field name="is_open"/>

              </tree>
          </field>
      </record>
          #action............
       <record id="tracks_action" model="ir.actions.act_window">
        <field name="name">tracks</field>
        <field name="res_model">iti.track</field>
     </record>
      #menus.............
      <menuitem id="iti_track" name="Tracks" parent="iti_root" action="tracks_action"/>

  </data>
</odoo>