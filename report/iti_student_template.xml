<odoo>


        <template  id="iti_student_template">
            <t t-call="web.html_container">
               <t t-foreach="docs" t-as="o">
                   <t t-call="web.external_layout">
                       <div class="page">
                          <h1>
                                 Name: <span t-field="o.name"/>
                          </h1>
                           <h1>
                                 mobile: <span t-field="o.mobile"/>
                           </h1>
                       </div>
                   </t>
               </t>
            </t>
        </template>
</odoo>